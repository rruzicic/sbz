import com.sbz.bank.model.User;
import com.sbz.bank.model.CreditRequest;

rule "Say hi when a new credit req comes up"
    when
        oldClientLongCredit()
    then
        System.out.println("Credit request will be modified");
end

query oldClientLongCredit()
    not CreditRequest(
        client.age >= 60,
        rateNumber >= 120
    )
end
